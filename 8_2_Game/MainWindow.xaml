<Window x:Class="_8_2_Game.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:_8_2_Game"
        xmlns:gif="http://wpfanimatedgif.codeplex.com"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Canvas Name="GameField">
        <Image x:Name="Pikachu" gif:ImageBehavior.AnimatedSource="/Images/Pikachu.gif" Tag="object" gif:ImageBehavior.RepeatBehavior="1x" gif:ImageBehavior.AnimationSpeedRatio="0.90"  Height="103" Canvas.Left="61"  Canvas.Top="206" Width="145" HorizontalAlignment="Left" VerticalAlignment="Center"/>
        <Image x:Name="energy" gif:ImageBehavior.AnimatedSource="/Images/energyBall.gif" Tag="object" gif:ImageBehavior.RepeatBehavior="0x" gif:ImageBehavior.AnimationSpeedRatio="1.00"  Height="75" Canvas.Left="-368"  Canvas.Top="191" Width="141"/>
        <Rectangle x:Name="Ground1" Tag="wall" Height="100" Stroke="Black" Canvas.Left="0" Canvas.Top="341" Width="800" HorizontalAlignment="Left" VerticalAlignment="Center" />
    </Canvas>
</Window>
